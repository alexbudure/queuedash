{
  "name": "@queuedash/api",
  "version": "2.0.5",
  "description": "A stunning, sleek dashboard for Bull, BullMQ, and Bee-Queue",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "pnpm run build --watch",
    "test": "pnpm run test:bull && pnpm run test:bullmq && pnpm run test:bee",
    "test:bull": "QUEUE_TYPE=bull vitest run",
    "test:bullmq": "QUEUE_TYPE=bull vitest run",
    "test:bee": "QUEUE_TYPE=bee vitest run",
    "lint": "eslint ./ --fix"
  },
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "types": "./dist/src/main.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "bull",
    "bee-queue",
    "queue",
    "bullmq",
    "dashboard"
  ],
  "dependencies": {
    "@trpc/server": "^10.45.0",
    "redis": "^4.6.13",
    "redis-info": "^3.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@elysiajs/trpc": "^0.8.0",
    "@faker-js/faker": "^8.4.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/node": "^20.11.16",
    "@types/redis-info": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bee-queue": "^1.7.1",
    "bull": "^4.12.2",
    "bullmq": "^5.1.9",
    "elysia": "^0.8.15",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.18.2",
    "fastify": "^4.26.0",
    "prettier": "^3.2.5",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
